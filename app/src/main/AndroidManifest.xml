<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthandfitnessapp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.HealthAndFitnessApp">
        <activity
            android:name=".WebViewActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".RemindersActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
        <activity
            android:name=".TodayTasksActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
        <activity
            android:name=".MacrosHelpActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
        <activity
            android:name=".CurrentDayActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:exported="false" />
        <activity
            android:name=".GettingStarted"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.MainActivity" />
        </activity>
        <activity
            android:name=".FirstTimeSignUp"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.MainActivity" />
        </activity>
        <activity
            android:name=".GymParkActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
        <activity
            android:name=".GroceryStoresActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HealthOptionsActivity" />
        </activity>
        <activity
            android:name=".SearchResultActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HealthOptionsActivity" />
        </activity>
        <activity
            android:name=".HealthOptionsActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.healthandfitnessapp.HomePage" />
        </activity>
    </application>

</manifest>